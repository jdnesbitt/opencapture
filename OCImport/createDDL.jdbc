CREATE TABLE batches (BATCH_ID BIGINT AUTO_INCREMENT NOT NULL, BATCH_STATE INTEGER NOT NULL, ERROR_NO INTEGER, BATCH_CLASS_ID BIGINT NOT NULL, ERROR_MSG TEXT(64000), SITE_ID INTEGER NOT NULL, BATCH_DESC VARCHAR(255), SCAN_DATETIME DATETIME NOT NULL, PRIORITY SMALLINT, BATCH_NAME VARCHAR(255) NOT NULL, QUEUE_ID BIGINT NOT NULL, PRIMARY KEY (BATCH_ID))
CREATE TABLE lkp_batchclass_queues (SEQUENCE_ID INTEGER NOT NULL, QUEUE_ID BIGINT NOT NULL, BATCH_CLASS_ID BIGINT NOT NULL, PRIMARY KEY (QUEUE_ID, BATCH_CLASS_ID))
CREATE TABLE batch_class (BATCH_CLASS_ID BIGINT AUTO_INCREMENT NOT NULL, BATCH_CLASS_NAME VARCHAR(255) NOT NULL, DESCR VARCHAR(255), IMAGE_PATH VARCHAR(255) NOT NULL, PRIMARY KEY (BATCH_CLASS_ID))
CREATE TABLE queues (QUEUE_ID BIGINT AUTO_INCREMENT NOT NULL, QUEUE_NAME VARCHAR(255) NOT NULL, QUEUE_DESC VARCHAR(255), PLUGIN VARCHAR(255), PRIMARY KEY (QUEUE_ID))
